<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pjpsystems.grameentest">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name=".architecture.application.GTApplication"
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.GrameenTest">
        <activity android:name=".ui.dashboard.InvitationSelectionActivity"></activity>
        <activity android:name=".ui.configuration.LoadScreenActivity" />
        <activity android:name=".ui.calendar.CalendarActivity" />
        <activity
            android:name=".ui.login.LoginActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>